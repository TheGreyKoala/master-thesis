grammar org.xtext.example.htmldsl.HtmlDsl with org.eclipse.xtext.common.Terminals

generate htmlDsl "http://www.xtext.org/example/htmldsl/HtmlDsl"

WebSite:
	(pages+=Page)+;

Page:
	'page' name=ID '{'
		(bodyElements+=BodyElement)+
	'}';

BodyElement:
	Heading1 | Heading2 | Paragraph | Anchor;

Heading1:
	'h1' '{'
		text=STRING
	'}';

Heading2:
	'h2' '{'
		text=STRING
	'}';

Paragraph:
	'p' '{'
		text=STRING
	'}';

Anchor:
	'a' '{'
		destination=[Page]
	'}';